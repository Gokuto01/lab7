node {
        stage('Welcome to master'){
                echo 'Start Program'
        }

        stage('Tigger the Slave Jobs') {
                echo 'Start Trigger to send name and age to slave'
                def props = readJSON file: 'data.json'
                sh "echo ${props.name}"
                sh "echo ${props.ages}"
                build job: 'jenkins_slave', parameters: [string(name: 'name', value: 'x'), string(name: 'ages', value: 'y')]
        }
}
