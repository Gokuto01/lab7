node() {
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
                def data = readJSON file: '/input.json'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'
                build job: 'jenkins_slave', parameters: [string(name: 'NAME', value: 'Jeeraphat'), string(name: 'AGE', value: '21')]
            }
        }
}
