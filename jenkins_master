pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
                sh 'python myscript.py Alice --age 30'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'
                build job: 'jenkins_slave', parameters: [string(name: 'NAME', value: 'Alice'), string(name: 'Ages', value: '21')]
            }
        }
    }
}
