pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'Alice', description: 'Enter your name')
        string(name: 'AGE', defaultValue: '21', description: 'Enter your age')
    }
    stages {
        stage('Example') {
            steps {
                echo "Hello from Slave ${params.NAME} of ${params.AGE}"
                 sh 'python myscript.py Alice --age 30'
            }
        }
    }
}
